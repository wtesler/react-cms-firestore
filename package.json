{
  "name": "react-cms-firestore",
  "version": "1.7.9",
  "description": "Dependency-inject CMS data into any react component from the Firestore",
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "build_and_publish": "npm run build && npm publish",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "react-content-loader": "^6.2.0"
  },
  "peerDependencies": {
    "firebase": "^9",
    "react": "^18"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/preset-react": "^7.16.7",
    "rimraf": "3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "browserslist": [
    ">0.25%",
    "not dead"
  ],
  "keywords": [
    "firebase",
    "firestore",
    "cms",
    "free"
  ],
  "author": "Will Tesler",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtesler/react-cms-firestore.git"
  },
  "homepage": "https://github.com/wtesler/react-cms-firestore"
}
