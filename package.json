{
  "name": "react-cms-firestore",
  "version": "1.0.6",
  "description": "Dependency-inject CMS data into any react component from the Firestore",
  "scripts": {
    "build": "npm run clean && babel src --out-dir dist --copy-files",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "firebase": "^9.0.0-beta.6",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "rimraf": "3.0.2"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": []
  },
  "keywords": [
    "firebase",
    "firestore",
    "cms",
    "free"
  ],
  "author": "Will Tesler",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/wtesler/react-cms-firestore.git"
  },
  "homepage": "https://github.com/wtesler/react-cms-firestore"
}
